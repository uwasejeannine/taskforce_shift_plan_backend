{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "babel": "babel index.js -d dist --presets @babel/preset-modules",
    "dev": "npm run babel && nodemon --exec babel-node index.js",
    "lint": "eslint --fix \"**/*.js\"",
    "format": "npm run lint && prettier --write \"**/*.js\"",
    "build": "rm -rf build && babel src -d build --presets @babel/preset-env",
    "start": "npm run build && node build/index.js",
    "migrate": "npx sequelize-cli db:migrate",
    "seed": "npx sequelize-cli db:seed:all",
    "migrate:reset": "npx sequelize-cli db:migrate:undo:all && npm run migrate",
    "test": "cross-env NODE_ENV=test jest --testTimeout=10000",
    "pretest": "cross-env NODE_ENV=test",
    "swagger-autogen": "node ./src/swagger/swagger.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.3.1",
    "email-validator": "^2.0.4",
    "eslint": "^8.45.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "express": "^4.18.2",
    "generate-password": "^1.7.1",
    "handlebars": "^4.7.8",
    "jest": "^29.7.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.6",
    "nodemon": "^3.0.1",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.33.0",
    "supertest": "^6.3.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.9",
    "@babel/core": "^7.23.0",
    "@babel/node": "^7.22.6",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-modules": "^0.1.6",
    "@babel/preset-react": "^7.22.15",
    "cross-env": "^7.0.3",
    "eslint": "^8.50.0",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "node-pre-gyp": "0.12.0",
    "prettier": "^3.0.3",
    "swagger-autogen": "^2.23.6"
  }
}
